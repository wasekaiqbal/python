ask=str(input("Lets learns all the months of the year. SHALL WE? \n Answer: "))

import calendar
from datetime import datetime 

if ask == "yes" :
    for month in range (1,13):
       all_months = calendar.month (2025,month)
       print(all_months)
       print("the name of the months are given below" )
       print(month)
       print("THANKS FOR THE CORRECTIONS!!!")
else:
    print("incorrect input")
